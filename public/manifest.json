{
    "manifest_version": 3,
    "name": "Fincor",
    "description": "This wallet makes it easy to access your crypto and interact with blockchain.",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html",
        "default_title": "Open the popup"
    },
    "icons": {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    },
    "permissions": ["storage"]
}
